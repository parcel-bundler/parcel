{
  "name": "@parcel/three-js-stresstest",
  "version": "2.0.0-beta.3",
  "license": "MIT",
  "private": true,
  "scripts": {
    "setup": "make src",
    "build": "rm -rf dist && parcel build src/entry.js --no-cache",
    "get-size": "du -sh dist"
  },
  "browserLegacy": "dist/legacy/index.js",
  "browserModern": "dist/modern/index.js",
  "targets": {
    "browserModern": {
      "engines": {
        "browsers": [
          "last 1 Chrome version"
        ]
      }
    },
    "browserLegacy": {
      "engines": {
        "browsers": [
          "> 0.25%"
        ]
      }
    }
  },
  "devDependencies": {
    "@parcel/config-default": "2.0.0-beta.3",
    "parcel": "2.0.0-beta.3"
  }
}
