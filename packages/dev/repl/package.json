{
  "name": "@parcel/repl",
  "version": "2.0.0-alpha.3.2",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "yarn copy-wasm && parcel2 src/index.html --no-source-maps",
    "build": "parcel2 build src/index.html --no-source-maps && yarn copy-wasm",
    "copy-wasm": "cp $(node -p 'require.resolve(\"@parcel/source-map/wasm-browser/source-map.wasm\")') dist/",
    "serve": "serve dist/ -c ../serve.json",
    "prebuild": "node write-commit.js"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^0.17.0",
    "@codemirror/closebrackets": "^0.17.0",
    "@codemirror/commands": "^0.17.0",
    "@codemirror/comment": "^0.17.0",
    "@codemirror/fold": "^0.17.0",
    "@codemirror/gutter": "^0.17.0",
    "@codemirror/highlight": "^0.17.0",
    "@codemirror/history": "^0.17.0",
    "@codemirror/lang-css": "^0.17.0",
    "@codemirror/lang-html": "^0.17.0",
    "@codemirror/lang-javascript": "^0.17.0",
    "@codemirror/lang-json": "^0.17.0",
    "@codemirror/language": "^0.17.1",
    "@codemirror/lint": "^0.17.0",
    "@codemirror/matchbrackets": "^0.17.0",
    "@codemirror/rectangular-selection": "^0.17.0",
    "@codemirror/search": "^0.17.0",
    "@codemirror/state": "^0.17.0",
    "@codemirror/theme-one-dark": "^0.17.1",
    "@codemirror/view": "^0.17.1",
    "@mischnic/codemirror-preact": "^0.1.0",
    "@mischnic/dot-svg": "^0.0.0",
    "@mischnic/pako": "^1.0.10",
    "@mischnic/yarn-browser": "^0.0.2",
    "@parcel/config-repl": "^2.0.0-alpha.3",
    "@parcel/core": "^2.0.0-alpha.3.2",
    "@parcel/fs": "^2.0.0-alpha.3",
    "@parcel/package-manager": "^2.0.0-alpha.3",
    "comlink": "^4.3.0",
    "filesize": "^6.1.0",
    "idb": "^5.0.8",
    "isomorphic-fetch": "^3.0.0",
    "preact": "^10.5.9",
    "semver": "^7.3.2",
