{
  "name": "@parcel/repl",
  "private": true,
  "version": "1.12.0",
  "description": "Blazing fast, zero configuration web application bundler",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/parcel-bundler/parcel.git"
  },
  "scripts": {
    "dev": "parcel build src/index.html --no-source-maps --no-content-hash --no-minify",
    "prod": "rm -rf dist/*; parcel build src/index.html --no-source-maps",
    "prod:shake": "rm -rf dist/*; NODE_OPTIONS='--max-old-space-size=2048' parcel build src/index.html --experimental-scope-hoisting",
    "deploy": "yarn prod && now && now alias"
  },
  "dependencies": {
    "@parcel/fs": "^1.11.0",
    "@parcel/logger": "^1.11.0",
    "codemirror": "^5.44.0",
    "comlink": "^3.2.0",
    "parcel-bundler": "^1.12.1",
    "preact": "next",
    "react-codemirror2": "^5.1.0",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4"
  },
  "alias": {
    "react": "preact",
    "@iarna/toml": "toml",
    "deasync": "./noop.js",
    "node-sass": "sass",
    "fs": "@parcel/fs/src/fs-native.js",
    "v8-compile-cache": "./noop.js"
  },
  "browserslist": [
    "Chrome 70"
  ]
}
