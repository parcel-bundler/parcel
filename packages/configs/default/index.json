{
  "bundler": "@parcel/bundler-default",
  "resolvers": {
    "*.{js,mjs,jsm,jsx,es6,ts,tsx,coffee}": [
      "@parcel/resolver-path",
      "@parcel/resolver-node"
    ],
    "*.{css,styl,stylus}": ["@parcel/resolver-url"],
    "*.{htm,html}": ["@parcel/resolver-url"]
  },
  "transforms": {
    "*.{js,mjs,jsm,jsx,es6,ts,tsx}": [
      "@parcel/transformer-babel",
      "@parcel/transformer-js"
    ],
    "*.{json,json5}": ["@parcel/transformer-json"],
    "*.{styl,stylus}": ["@parcel/transformer-stylus"],
    "*.less": ["@parcel/transformer-less"],
    "*.css": ["@parcel/transformer-css", "@parcel/transformer-postcss"],
    "*.{htm,html}": [
      "@parcel/transformer-posthtml",
      "@parcel/transformer-html"
    ],
    "*.coffee": ["@parcel/transformer-coffeescript"],
    "*": ["@parcel/transformer-raw"]
  },
  "namers": ["@parcel/namer-default"],
  "runtimes": {
    "browser": ["@parcel/runtime-js", "@parcel/runtime-browser-hmr"],
    "node": ["@parcel/runtime-js"]
  },
  "optimizers": {
    "*.css": ["@parcel/optimizer-cssnano"],
    "*.js": ["@parcel/optimizer-terser"]
  },
  "packagers": {
    "*.html": "@parcel/packager-html",
    "*.css": "@parcel/packager-css",
    "*.js": "@parcel/packager-js",
    "*": "@parcel/packager-raw"
  },
  "reporters": [
    "@parcel/reporter-cli",
    "@parcel/reporter-dev-server",
    "@parcel/reporter-hmr-server"
  ]
}
